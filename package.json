{
  "name": "kdfamily-chore-system",
  "version": "1.0.0",
  "description": "Family Chore Scoring System Web Application",
  "main": "next.config.js",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "dev:frontend": "next dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "bun test",
    "build": "next build",
    "build:frontend": "next build",
    "dev:client": "next dev --turbo",
    "dev:client:legacy": "next dev",
    "start:client": "next start",
    "export": "next export",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint app/ components/ store/ services/ --ext .js,.ts,.tsx",
    "lint:fix": "eslint app/ components/ store/ services/ --ext .js,.ts,.tsx --fix",
    "lint:next": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"app/**/*.{ts,tsx,js,jsx,css,md}\"",
    "wait-for-services": "node scripts/wait-for-services.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:test": "docker-compose -f docker-compose.dev.yml --profile testing up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "caddy:dev": "./scripts/deploy-caddy.sh dev",
    "caddy:prod": "./scripts/deploy-caddy.sh prod",
    "caddy:logs": "./scripts/deploy-caddy.sh logs caddy",
    "caddy:health": "./scripts/deploy-caddy.sh health",
    "caddy:restart": "./scripts/deploy-caddy.sh restart caddy",
    "start:dev": "./scripts/start-dev.sh",
    "start:prod": "./scripts/start-prod.sh",
    "health-check": "./scripts/health-check.sh"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.4.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.52.0",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.460.0",
    "next": "^15.4.2",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.0",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.31.0",
    "@module-federation/nextjs-mf": "^8.8.33",
    "@playwright/test": "^1.49.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/pg": "^8.15.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.12.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jest-html-reporters": "^3.1.7",
    "nodemon": "^3.0.2",
    "playwright": "^1.49.1",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.3",
    "webpack": "^5.100.2"
  },
  "keywords": [
    "family",
    "chore",
    "scoring",
    "task-management",
    "nextjs",
    "react",
    "rust",
    "sqlite"
  ],
  "author": "KD Family",
