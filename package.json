{
  "name": "kdfamily-chore-system",
  "version": "1.0.0",
  "description": "Family Chore Scoring System Web Application",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "dev:debug": "nodemon --inspect=0.0.0.0:9229 server/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "build": "echo 'No build process needed - using static files from public directory'",
    "lint": "eslint server/ --ext .js",
    "lint:fix": "eslint server/ --ext .js --fix",
    "type-check": "echo 'Type checking not implemented yet'",
    "migrate:dev": "node scripts/migrate.js",
    "migrate:test": "NODE_ENV=test node scripts/migrate.js",
    "seed:dev": "node scripts/seed.js",
    "wait-for-services": "node scripts/wait-for-services.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:test": "docker-compose -f docker-compose.dev.yml --profile testing up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "caddy:dev": "./scripts/deploy-caddy.sh dev",
    "caddy:prod": "./scripts/deploy-caddy.sh prod",
    "caddy:logs": "./scripts/deploy-caddy.sh logs caddy",
    "caddy:health": "./scripts/deploy-caddy.sh health",
    "caddy:restart": "./scripts/deploy-caddy.sh restart caddy"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.7",
    "validator": "^13.15.15",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "nodemon": "^3.0.2",
    "redis": "^4.6.5",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "family",
    "chore",
    "scoring",
    "task-management",
    "nodejs",
    "mysql"
  ],
  "author": "KD Family",
  "license": "MIT"
}
